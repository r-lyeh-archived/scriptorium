@[test(code_01)]
class IntList
{
	var ints = {}

	routine Append( value : int ){ ints.append( value ) }
	routine []( index : int ){ return ints[index] }
	routine []=( value :int, index : int ){
		ints[index] = value
		io.writeln( "set item" )
	}
}
ilist = IntList()
ilist.Append( 123 )
ilist.Append( 456 )
ilist[0] = 789
io.writeln( ilist[0] )
@[test(code_01)]
