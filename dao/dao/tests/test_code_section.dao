@[test(code_00)]
routine First()[int]
{
	io.writeln( "Calling First()" )
	for( i = 1 : 5 ) yield(i)
}

routine Second()[int]
{
	io.writeln( "Calling Second()" )
	First{ [X]
		io.writeln( "Yielding from Second()" )
		yield(X)
	}
}

Second{ [X] io.writeln( X ) } 
@[test(code_00)]
@[test(code_00)]
Calling Second()
Calling First()
Yielding from Second()
1
Yielding from Second()
2
Yielding from Second()
3
Yielding from Second()
4
Yielding from Second()
5
@[test(code_00)]





