	child = % utp.snamespace ? utp.snamespace + "::" + utp.name : utp.name;
	kvmap0 = { "type"=>utp.name, "child"=>child, "parent"=>"", "parent2"=>"" };
	for( sup in utp.supers ){
		parents += "dao_" + sup.name + "_Typer, ";
