		DaoCdata *it = DaoValue_CastCdata( DaoList_GetItem( dlist, i ) );
		if( it == NULL || ! DaoCdata_IsType( it, dao_$(item)_Typer ) ) continue;
		qlist.append( *($(item)*) DaoCdata_CastData( it, dao_$(item)_Typer ) );
	}
}
