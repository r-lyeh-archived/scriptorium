			f = f.trim();
			if( tmps.find( f ) != none ) skip;
			tmps[f] = 1;
			if( % f ){
				files.append( f );
				extfiles[ f ] = 1;
			}
		}
		tmps = {=>};
		for( f in cate[1].split( "," ) ){
			f = f.trim();
			if( tmps.find( f ) != none ) skip;
			tmps[f] = 1;
