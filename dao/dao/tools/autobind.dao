		tmps = {=>};
		for( f in cate[0].split( "," ) ){
			f = f.trim();
			if( tmps.find( f ) != none ) skip;
			tmps[f] = 1;
			if( % f ) files.append( f );
		}
	}else if( cate.size() ==2 ){
		tmps = {=>};
