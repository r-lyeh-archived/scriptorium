routine BernsteinHash( key : string )
{
	hash = 0;
	for( i = 0 : key.size()-1 ) hash = 33*hash + key[i];
	return hash;
}
routine RotatingHash( key : string )
{
	n = key.size();
	hash = n;
	for( i = 0 : n-1 ) hash = ((hash<<4)^(hash>>28)^key[i])&0x7fffffff;
	return hash % 997;
}

