				tpl_cxx2dao = cxx2dao_voidp;
				if( ispointer ){
					tpl_dao2cxx = dao2cxx_void2;
					tpl_cxx2dao = cxx2dao_user;
				}
				if( vdefault.match( "= %s* (0|NULL)" ) != none ) vdefault = "|none=none";
