A = vec2( 1.0, 2.0 );
print( "A: ", A, "\n" );
B = vec2( 3.0, 3.0 );
print( "B: ", B, "\n" );
print( "A + B: ", A + B, "\n" );
print( "A - B: ", A - B, "\n" );
print( "A * B: ", A * B, "\n" );
print( "A / B: ", A / B, "\n" );
print( "B % A: ", B % A, "\n" );
print( "-A: ", -A, "\n" );
print( "A == B: ", A == B, "\n" );
print( "A < B: ", A < B, "\n" );
print( "A == A: ", A == A, "\n" );
print( "A < A: ", A < A, "\n" );
A( "cheese" );
print( typeof( A ) );
